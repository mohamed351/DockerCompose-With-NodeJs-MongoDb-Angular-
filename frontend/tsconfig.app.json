{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [
      "src/**/*.d.ts"
    ],
    "typeRoots": [
      "src/**/*.d.ts"
    ]
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts",

  ],
  "include": [
    "src/**/*.d.ts",
    "src/typing.d.ts"
  ]
}
